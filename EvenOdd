import java.util.*;
public class EvenOdd {
    public static void main(String [] args){
        int x;
        System.out.println("Enter your number");
        Scanner sc = new Scanner(System.in);
         x = sc.nextInt();
                if(x%2==0){
            System.out.println("Even number");

        }else{
            System.out.println("Odd number");
        }

    }
}
